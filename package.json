{
  "name": "@cloudfour/eslint-plugin",
  "version": "11.0.0",
  "description": "Cloud Four's shareable ESLint configuration",
  "author": "Cloud Four <info@cloudfour.com> (http://cloudfour.com)",
  "homepage": "https://github.com/cloudfour/eslint-config",
  "bugs": "https://github.com/cloudfour/eslint-config/issues",
  "license": "MIT",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudfour/eslint-config.git"
  },
  "contributors": [
    "Erik Jung <erik@cloudfour.com>",
    "Caleb Eby <caleb@cloudfour.com>"
  ],
  "files": [
    "dist/config.js",
    "src/rules.js",
    "src/rules/**/*.js",
    "!src/rules/**/*.test.js",
    "index.js"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "dependencies": {
    "@babel/core": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^27.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^20.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@cloudfour/eslint-plugin": ".",
    "eliminate": "1.1.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-xo": "0.31.0",
    "eslint-formatter-pretty": "3.0.1",
    "jest": "26.0.1",
    "jest-circus": "26.0.1",
    "kleur": "3.0.3",
    "mkdirplz": "1.0.2",
    "powerwalker": "0.1.2",
    "prettier": "2.0.5",
    "prompts": "2.3.2"
  },
  "scripts": {
    "check-lint": "eslint --format=pretty . && prettier --check .",
    "load-fixture-repo": "node fixtures/load-repo",
    "lint": "eslint --format=pretty --fix . && prettier --write .",
    "prepublishOnly": "npm ci && npm run build && npm run check-lint",
    "postpublish": "git tag v$npm_package_version && git push --tags",
    "build": "node build.js",
    "test": "jest"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "plugin:@cloudfour/recommended"
  },
  "jest": {
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner"
  }
}
